{
  "name": "vuetodo",
  "version": "1.0.0",
  "description": "a todo app of vue",
  "main": "index.js",
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "build:client": "cross-env NODE_ENV=production webpack --config build/webpack.config.client.js",
    "build:server": "cross-env NODE_ENV=production webpack --config build/webpack.config.server.js",
    "build": "npm run clean && npm run build:client && npm run build:server",
    "clean": "rimraf public && rimraf server-build",
    "lint": "eslint --ext .js --ext .jsx --ext .vue client/",
    "lint-fix": "eslint --fix --ext .js --ext .jsx --ext .vue client/",
    "precommit": "npm run lint-fix",
    "dev:client": "cross-env NODE_ENV=development webpack-dev-server --config build/webpack.config.client.js",
    "dev:server": "nodemon server/server.js",
    "dev": "concurrently \"npm run dev:client\" \"npm run dev:server\"",
    "start": "cross-env NODE_ENV=production node server/server.js"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/chu8ug/vue-todo.git"
  },
  "author": "chu8ug",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/chu8ug/vue-todo/issues"
  },
  "homepage": "https://github.com/chu8ug/vue-todo#readme",
  "dependencies": {
    "axios": "^0.19.0",
    "ejs": "^2.7.4",
    "koa": "^2.11.0",
    "koa-body": "^4.1.1",
    "koa-router": "^7.4.0",
    "koa-send": "^5.0.0",
    "koa-session": "^5.12.3",
    "sha1": "^1.1.1",
    "vue": "^2.6.11",
    "vue-meta": "^2.3.1",
    "vue-router": "^3.1.3",
    "vue-server-renderer": "^2.6.11",
    "vuex": "^3.1.2"
  },
  "devDependencies": {
    "@babel/cli": "^7.7.5",
    "@babel/core": "^7.7.5",
    "@babel/plugin-syntax-dynamic-import": "^7.7.4",
    "@babel/plugin-syntax-jsx": "^7.0.0",
    "@babel/preset-env": "^7.7.6",
    "@vue/babel-plugin-transform-vue-jsx": "^1.1.2",
    "autoprefixer": "^9.7.3",
    "babel-eslint": "^10.0.3",
    "babel-helper-vue-jsx-merge-props": "^2.0.3",
    "babel-loader": "^8.0.6",
    "concurrently": "^5.0.1",
    "cross-env": "^6.0.3",
    "css-loader": "^3.3.2",
    "eslint": "^5.16.0",
    "eslint-config-standard": "^12.0.0",
    "eslint-loader": "^3.0.3",
    "eslint-plugin-html": "^6.0.0",
    "eslint-plugin-import": "^2.19.1",
    "eslint-plugin-node": "^10.0.0",
    "eslint-plugin-promise": "^4.2.1",
    "eslint-plugin-standard": "^4.0.1",
    "eslint-plugin-vue": "^5.2.3",
    "file-loader": "^4.3.0",
    "html-webpack-plugin": "^5.5.0",
    "memory-fs": "^0.5.0",
    "mini-css-extract-plugin": "^0.8.0",
    "nodemon": "^1.19.4",
    "postcss-loader": "^3.0.0",
    "rimraf": "^3.0.0",
    "style-loader": "^1.0.1",
    "stylus": "^0.54.7",
    "stylus-loader": "^3.0.2",
    "url-loader": "^2.3.0",
    "vue-loader": "^15.8.3",
    "vue-style-loader": "^4.1.2",
    "vue-template-compiler": "^2.6.11",
    "webpack": "^4.41.3",
